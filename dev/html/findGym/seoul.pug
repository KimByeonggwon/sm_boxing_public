	script(src=`//dapi.kakao.com/v2/maps/sdk.js?appkey=cbf134aa1e5c01a91daa6d20263cb77f`, type='text/javascript', charset='utf-8')
	script.
		$(document).ready(function(){
			
			
			 $('.ui-slider-for').slick({
				slidesToShow: 1,
				slidesToScroll: 1,
				arrows: false,
				fade: true,
				setPosition : true,
				asNavFor: '.ui-slider-nav'
			});
			$('.ui-slider-nav').slick({
				slidesToShow: 4,
				slidesToScroll: 1,
				asNavFor: '.ui-slider-for',
				setPosition : true,
				focusOnSelect: true,
				responsive:[
					{
						breakpoint: 1024,
						settings: {
							arrows: false,
							slidesToShow: 2
						}
					}
				]
			});


			// 1관
			var container1 = document.getElementById('map1');
			var options1 = {
				center: new kakao.maps.LatLng(37.53611899611797, 126.86374733091611),
				level: 3
			};		

			// 2관
			var container2 = document.getElementById('map2');
			var options2 = {
				center: new kakao.maps.LatLng(37.548469045815835, 126.86616011591776),
				level: 3
			};

			// 3관
			var container3 = document.getElementById('map3');
			var options3 = {
				center: new kakao.maps.LatLng(37.525927567070035, 126.86124536212441),
				level: 3
			};

