				center: new kakao.maps.LatLng(37.53722162216685, 127.1233153292424),
				level: 3
			};

			// 10관
			var container10 = document.getElementById('map10');
			var options10 = {
				center: new kakao.maps.LatLng(37.449377280998384, 126.91344080763992),
				level: 3
			};

			var map1 = new kakao.maps.Map(container1, options1);
			var map2 = new kakao.maps.Map(container2, options2);
			var map3 = new kakao.maps.Map(container3, options3);
			var map9 = new kakao.maps.Map(container9, options9);
			var map10 = new kakao.maps.Map(container10, options10);

			// 마커가 표시될 위치입니다 
			var markerPosition1  = new kakao.maps.LatLng(37.53611899611797, 126.86374733091611); 
			var markerPosition2  = new kakao.maps.LatLng(37.548469045815835, 126.86616011591776); 
			var markerPosition3  = new kakao.maps.LatLng(37.525927567070035, 126.86124536212441); 
			var markerPosition9  = new kakao.maps.LatLng(37.53722162216685, 127.1233153292424); 
			var markerPosition10  = new kakao.maps.LatLng(37.449377280998384, 126.91344080763992);


			// 마커를 생성합니다
			var marker1 = new kakao.maps.Marker({
					position: markerPosition1
			});
			var marker2 = new kakao.maps.Marker({
					position: markerPosition2
			});
			var marker3 = new kakao.maps.Marker({
					position: markerPosition3
			});
			var marker9 = new kakao.maps.Marker({
					position: markerPosition9
			});
			var marker10 = new kakao.maps.Marker({
					position: markerPosition10
			});

			// 마커가 지도 위에 표시되도록 설정합니다
			marker1.setMap(map1);
			marker2.setMap(map2);
			marker3.setMap(map3);
			marker9.setMap(map9);
			marker10.setMap(map10);
		});
